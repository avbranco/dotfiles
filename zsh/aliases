# GIT
alias gp='git pull'
alias gf='git fetch'
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gcm='git commit -m '
alias gd='git diff'
alias go='git checkout '
alias gob='git checkout -b '
alias gk='gitk --all&'
alias gx='gitx --all'
alias gl='git log --graph --abbrev-commit --date=relative'

#RAILS3
alias rc="pry"
alias zc="zeus console"
alias rt='bundle exec ruby -Ilib:test'
alias zt='zeus test'
alias r='rails'
alias z='zeus'

#REDIS
alias redis-start='launchctl load -w ~/Library/LaunchAgents/homebrew.mxcl.redis.plist'
alias redis-stop='launchctl unload ~/Library/LaunchAgents/homebrew.mxcl.redis.plist'

#RP
alias rps='RP2SERVER=1 RAILS_ENV=development RPDEBUG=true rails runner "RP2Server.start(1)"'
alias rps2='RP2SERVER=1 RAILS_ENV=development RPDEBUG=true rails runner "RP2Server.start(2)"'
alias gorps='cd ~/work/replaygaming/replaypoker/vendor/plugins/rp2server/'
alias gorp='cd ~/work/replaygaming/replaypoker/'
alias rp-ssh='~/work/dotfiles/scripts/rp-servers.sh'
alias rp='~/work/dotfiles/scripts/rp-code.sh'
alias ec2s1='ec2-describe-instances | grep `script/ec2/name staging1`'
alias ec2s2='ec2-describe-instances | grep `script/ec2/name staging2`'

#TMUX
alias tmux="TERM=screen-256color-bce tmux"
