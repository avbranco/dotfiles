# GIT
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'

#RAILS 2
alias rcp='RAILS_ENV=production script/console --irb=pry'
alias rcd='RAILS_ENV=development script/console --irb=pry'
alias rcs='RAILS_ENV=staging script/console --irb=pry'

#GENERAL
alias fn='find . -name $1'
